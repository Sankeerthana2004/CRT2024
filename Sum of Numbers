#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n,sum=0;
   for(;;)       //while(1)
   {
       scanf("%d",&n);
       if(n==-1)
       {
           break;
       }
       sum=sum+n;
   }
    printf("%d",sum);
    return 0;
}
