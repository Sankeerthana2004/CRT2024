#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   int a=7,b=5,i,n;
    scanf("%d",&n);
    printf("%d %d",a,b);
    for(i=3;i<=n;i++)
    {
        if(i%2!=0)
        {
            a++;
            printf(" %d",a);
        }
        else
        {
            b++;
            printf(" %d",b);
        }
    }
    return 0;
}
